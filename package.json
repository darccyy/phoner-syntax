{
  "name": "phoner-syntax",
  "displayName": "phoner-syntax",
  "description": "Syntax Highlighting for Phoner",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "phoner",
      "aliases": ["Phoner"],
      "extensions": ["phoner", "phn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "phoner",
      "scopeName": "source.phoner",
      "path": "./syntaxes/phoner.tmLanguage.json"
    }]
  }
}
